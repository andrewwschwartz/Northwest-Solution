
@{
    ViewBag.Title = "Tab";
}
@Styles.Render("~/Content/css")

<h2>Tableau Quickview</h2>

<body style="background: linear-gradient(90deg,rgba(255,255,255,.1) 0,rgba(255,255,255,.1) 100%),url(../img/bg-masthead.jpg);">
    <div class="container">

        <div>            
            <div class='tableauPlaceholder myGraph' id='viz1490032193847' style='position: relative; height: 500px; width: 600px'>
                <noscript>
                    <a href='#'>
                        <img alt='Sheet 2 ' src='https:&#47;&#47;public.tableau.com&#47;static&#47;images&#47;NT&#47;NTJT5RDTK&#47;1_rss.png' style='border: none' />
                    </a>
                </noscript>
                <object class='tableauViz' style='display:none;'>
                    <param name='host_url' value='https%3A%2F%2Fpublic.tableau.com%2F' /> 
                    <param name='path' value='shared&#47;NTJT5RDTK' /> 
                    <param name='toolbar' value='yes' />
                    <param name='static_image' value='https:&#47;&#47;public.tableau.com&#47;static&#47;images&#47;NT&#47;NTJT5RDTK&#47;1.png' /> 
                    <param name='animate_transition' value='yes' />
                    <param name='display_static_image' value='yes' />
                    <param name='display_spinner' value='yes' />
                    <param name='display_overlay' value='yes' />
                    <param name='display_count' value='yes' />
                </object>
            </div>
            <script type='text/javascript'>

                var divElement = document.getElementById('viz1490032193847');
                var vizElement = divElement.getElementsByTagName('object')[0];

                vizElement.style.width = '100%';
                vizElement.style.height = (divElement.offsetWidth * 0.75) + 'px';
                var scriptElement = document.createElement('script');
                scriptElement.src = 'https://public.tableau.com/javascripts/api/viz_v1.js';
                vizElement.parentNode.insertBefore(scriptElement, vizElement);
            </script>
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-md-3 text-center">
            @Html.ActionLink("Add Employee", "CreateEmployee", null, null, new { @class = "btn btn-primary btn-xl" })
        </div>
        <div class="col-md-3 text-center">
            @Html.ActionLink("View Work Orders", "ViewWO", null, null, new { @class = "btn btn-primary btn-xl" })
        </div>
        <div class="col-md-3 text-center">
            @Html.ActionLink("Create Work Order", "CreateWO", null, null, new { @class = "btn btn-primary btn-xl" })
        </div>
        <div class="col-md-3 text-center">
            @Html.ActionLink("Edit Work Order", "EditWO", null, null, new { @class = "btn btn-primary btn-xl" })
        </div>
    </div>
    
</body>