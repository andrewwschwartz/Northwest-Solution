
@{
    ViewBag.Title = "My Orders";
}

    <div class="main_container">
        <!-- page content -->
        <div role="main">
            <div class="">
                <div class="clearfix"></div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="x_panel">
                            <div class="x_title">
                                <br />
                                <h1 id="meow"><strong>Tylenol Tech</strong> <small>(968-34567)</small></h1>
                                <div class="clearfix"></div>
                            </div>
                            <div class="x_content">
                                <br />
                                <h3>Work Orders</h3>

                                <!-- start project list -->
                                <table class="table table-striped dt-responsive nowrap" cellspacing="0" width="100%">
                                    <thead>
                                        <tr>
                                            <th style="width: 10%">Order #</th>
                                            <th style="width: 20%">Compound Name</th>
                                            <th>Start Date</th>
                                            <th>Project Progress</th>
                                            <th>Status</th>
                                            <th>Assay Name</th>
                                            <th style="width: 18%"></th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>434882</td>
                                            <td>Calcium Gluconate</td>
                                            <td>10/28/2017</td>
                                            <td class="project_progress">
                                                <div class="progress progress_sm">
                                                    <div class="progress-bar bg-green" role="progressbar" data-transitiongoal="57"></div>
                                                </div>
                                                <small>57% Complete</small>
                                            </td>
                                            <td>
                                                <button type="button" class="btn btn-primary btn-xs">Back Order</button>
                                            </td>
                                            <td>CustomScreen</td>
                                            <td>
                                                <a href="/Client/Details" class="btn btn-primary btn-xs"><i class="fa fa-folder"></i> Details </a>
                                                <a href="#" class="btn btn-danger btn-xs"><i class="fa fa-trash-o"></i> Cancel </a>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>243556</td>
                                            <td>Abraxane</td>
                                            <td>11/22/2017</td>
                                            <td class="project_progress">
                                                <div class="progress progress_sm">
                                                    <div class="progress-bar bg-green" role="progressbar" data-transitiongoal="47"></div>
                                                </div>
                                                <small>47% Complete</small>
                                            </td>
                                            <td>
                                                <button type="button" class="btn btn-info btn-xs">In Progress</button>
                                            </td>
                                            <td>CustomScreen</td>
                                            <td>
                                                <a href="/Client/Details" class="btn btn-primary btn-xs"><i class="fa fa-folder"></i> Details </a>
                                                <a href="#" class="btn btn-danger btn-xs"><i class="fa fa-trash-o"></i> Delete </a>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>984561</td>
                                            <td>Ranitidine</td>
                                            <td>11/30/2017</td>
                                            <td class="project_progress">
                                                <div class="progress progress_sm">
                                                    <div class="progress-bar bg-green" role="progressbar" data-transitiongoal="77"></div>
                                                </div>
                                                <small>77% Complete</small>
                                            </td>
                                            <td>
                                                @{
                                                    if (ViewBag.WasApproved == true)
                                                    {
                                                        <button type="button" class="btn btn-info btn-xs">In Progress</button>
                                                    }
                                                    else
                                                    {
                                                        @Html.ActionLink("Approval Pending", "Details", null, null, new { @class = "btn btn-danger btn-xs" })
                                                    }
                                                }
                                            </td>
                                            <td>CustomScreen</td>
                                            <td>
                                                <a href="/Client/Details" class="btn btn-primary btn-xs"><i class="fa fa-folder"></i> Details </a>
                                                <a href="#" class="btn btn-danger btn-xs"><i class="fa fa-trash-o"></i> Delete </a>
                                            </td>
                                        </tr>

                                    </tbody>
                                </table>
                                <!-- end project list -->

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- /page content -->
    </div>


<!-- Bootstrap core CSS -->

<link href="~/gentelella/documentation/css/bootstrap.min.css" rel="stylesheet">

<link href="~/gentelella/production/fonts/css/font-awesome.min.css" rel="stylesheet">
<link href="~/gentelella/production/css/animate.min.css" rel="stylesheet">

<!-- Custom styling plus plugins
<link href="~/gentelella/production/css/custom.css" rel="stylesheet">-->
<link href="~/gentelella/production/css/icheck/flat/green.css" rel="stylesheet">

<script src="~/gentelella/production/js/jquery.min.js"></script>

<script src="~/gentelella/production/js/bootstrap.min.js"></script>

<!-- bootstrap progress js -->
<script src="~/gentelella/production/js/progressbar/bootstrap-progressbar.min.js"></script>
<script src="~/gentelella/production/js/nicescroll/jquery.nicescroll.min.js"></script>
<!-- icheck -->
<script src="~/gentelella/production/js/icheck/icheck.min.js"></script>

<script src="~/gentelella/production/js/custom.js"></script>

<!-- pace -->
<script src="~/gentelella/production/js/pace/pace.min.js"></script>
<div class="row">
    <div class="col-sm-offset-5 col-sm-1">
        <p>@Html.ActionLink("Back", "Index", "Client", null, new { @class = "btn btn-primary btn-primary2 btn-medium" })</p>
    </div>
    <div class="col-sm-1">
        <p>@Html.ActionLink("History", "History", "Client", null, new { @class = "btn btn-primary btn-primary2 btn-medium" })</p>
    </div>
</div>

